<FIMSAMECards xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../schema/FIMS-AME.xsd">
	<AMECard>
		<CardID>V010</CardID>
		<Name>Face recognition training</Name>
		<Type>Video</Type>
		<Status>draft</Status>
		<Editor>Lauri</Editor>
		<Definition>Training module for a face recognition system.</Definition>
		<Parameters>
			<Inputs>
				<Input mandatory="true">
					<Name>ClassificationData</Name>
					<Type>xml</Type>
				</Input>
				<Input>
					<Name>BoundingBox</Name>
					<Type>text</Type>
					<Definition>A bounding box, specified as space separated list of x,y,w,h in pixels.</Definition>
				</Input>
			</Inputs>
			<Outputs>
				<Output mandatory="true">
					<Name>WasTrainingSuccess</Name>
					<Type>text</Type>
					<Representation/>
				</Output>
			</Outputs>
		</Parameters>
		<CapabilityGroup name="supported face size">
			<Capability>
				<Values>urn:jhfkjsdhs</Values>
				<Type>minimum object size</Type>
				<Unit>pixel</Unit>
			</Capability>
		</CapabilityGroup>
		<CapabilityGroup name="input formats">
			<Capability>
				<Values>urn:cvds</Values>
				<Type>supported video formats</Type>
			</Capability>
			<Capability>
				<Values>urn:sdhfsdjkfh</Values>
				<Type>supported image formats</Type>
			</Capability>
		</CapabilityGroup>
	</AMECard>
</FIMSAMECards>
