<FIMSAMECards xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../schema/FIMS-AME.xsd">
	<AMECard>
		<CardID>T002</CardID>
		<Name>Lemmatization</Name>
		<Type>Text</Type>
		<Status>draft</Status>
		<Editor>Mike</Editor>
		<Definition>The identification of the most relevant words in a text.</Definition>
		<Parameters>
			<Inputs>
			</Inputs>
			<Outputs>
				<Output mandatory="true">
					<Name>StemmedWords</Name>
					<Type>xml</Type>
					<Representation>urn:xyz</Representation>
					<Definition>Each word in the input text in its stemmed form together with morphosyntactic information and a confidence value</Definition>
				</Output>
			</Outputs>
		</Parameters>
		<CapabilityGroup name="Text Types">
			<Capability>
				<Values>urn:oiweuowieru</Values>
				<Type>supported languages</Type>
			</Capability>
		</CapabilityGroup>
		<CapabilityGroup name="input formats">
			<Capability>
				<Values>urn:cvds</Values>
				<Type>supported text formats</Type>
			</Capability>
		</CapabilityGroup>

	</AMECard>
</FIMSAMECards>
